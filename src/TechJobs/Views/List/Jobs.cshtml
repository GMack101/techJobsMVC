<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>


    @foreach (var positions in ViewBag.jobs)
    {
        <Table class="job-listing">
            @foreach (KeyValuePair<string, string> kvp in positions)
            {
                <tr>
                    <td>@kvp.Key : @kvp.Value </td>
                </tr>

            }
            <tr>
                <td><br></td>

            </tr>
        </Table>
    }


