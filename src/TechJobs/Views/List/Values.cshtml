<h1>@ViewBag.title</h1>

<Table class="job-listing">
    @foreach (Dictionary<string, string> jobs in ViewBag.jobs)
    {
        @foreach (KeyValuePair<string, string> kvp in jobs)
        {
            <tr>
                <td>@kvp.Key : @kvp.Value </td>
            </tr>

        }
        <tr>
            <td><br></td>
        </tr>
    }
</Table>
